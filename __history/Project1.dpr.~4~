program Project1;

uses
  Vcl.Forms,
  TelaPrincipal in 'TelaPrincipal.pas' {Form1},
  CadsProdutos in 'Cadastros\CadsProdutos.pas' {CadProdutos},
  CadsCliente in 'Cadastros\CadsCliente.pas' {CadCliente},
  CadsCep in 'Cadastros\CadsCep.pas' {CadastroCep},
  PesqProdutos in 'Cadastros\view\PesqProdutos.pas' {PesqsProdutos},
  PesqClientes in 'Cadastros\view\PesqClientes.pas' {PesqsCliente},
  PesqCeps in 'Cadastros\view\PesqCeps.pas' {PesqsCeps},
  Banco in 'DB\Banco.pas' {DataModule1: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TCadProdutos, CadProdutos);
  Application.CreateForm(TCadCliente, CadCliente);
  Application.CreateForm(TCadastroCep, CadastroCep);
  Application.CreateForm(TPesqsProdutos, PesqsProdutos);
  Application.CreateForm(TPesqsCliente, PesqsCliente);
  Application.CreateForm(TPesqsCeps, PesqsCeps);
  Application.CreateForm(TDataModule1, DataModule1);
  Application.Run;
end.
